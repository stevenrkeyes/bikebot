<script type="text/javascript">
	if (!isBrowserSupported()) {
		alert('Your browser version is not supported.  For Netscape please use version 7 or above.');
	}
	function getParameterByName(name) {
		name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
		var regexS = "[\\?&]" + name + "=([^&#]*)";
		var regex = new RegExp(regexS);
		var results = regex.exec(window.location.search);
		if (results == null) {
			return "";
		} else {
			return decodeURIComponent(results[1].replace(/\+/g, " "));
		}
	}
	var affiliatePID = getParameterByName('PID');
	if (affiliatePID) {
		setCookie('AffiliatePID_10052', affiliatePID.toUpperCase());
	}
</script>
<!-- BEGIN TeamMemberCheck.jsp -->
<script type="text/javascript">
// sets the cookie with the specified name, expiration, path, and domain
function setCookie(name, value, expires, path, domain, secure)
{
  document.cookie = name + "=" + escape (value) +
    ((expires) ? "; expires=" + expires.toGMTString() : "") +
    ((path) ? "; path=" + path : "; path=/") +
    ((domain) ? "; domain=" + domain : "") +
    ((secure) ? "; secure" : "");
}

function getCookie(cookieName)
{
  var results = document.cookie.match ( '(^|;) ?' + cookieName + '=([^;]*)(;|$)' );
  if (results) {
    return (unescape (results[2]));
  } else {
    return null;
  }
}

// set expiry date for 1 hour from now
var d = new Date();
d.setTime(d.getTime() + (60*60*1000));
var v = getCookie('teamMember');
if (!v || v != 'N')
{
	// create or update the cookie
	setCookie('teamMember', 'N', d, '/', null, null);
}

</script>

