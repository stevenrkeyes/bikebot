		<script type="text/javascript">
		$(document).ready(function() {
			shrink = 0;			
			if (ghTrim($("#snShopByBrandBrands4").html()) == "") {
				shrink = 105;
				$("#snShopByBrandBrands4").remove();
				if (ghTrim($("#snShopByBrandBrands3").html()) == "") {
					shrink = 210;
					$("#snShopByBrandBrands3").remove();
					if (ghTrim($("#snShopByBrandBrands2").html()) == "") {
						shrink = 315;
						$("#snShopByBrandBrands2").remove();
					}
				}
			}
			if (shrink != 0) {
				$("#snShopByBrandPanel").css("width", 448 - shrink); 
				$("#snShopByBrandViewAll").css("left", 330 - shrink); 
				$("#snShopByBrandViewAllChevron").css("left", 421 - shrink); 
			}
			
			$("#snShopByBrand").mouseenter(function() {
				ghMenuClearTimeout();
				ghHideAllMenus();
				$("#snShopByBrand").addClass("snMenu_on");
				$("#snShopByBrandPanel").css("visibility", "visible");
			});
			$("#snShopByBrand").mouseleave(ghMenuSetTimeout);
			$("#snShopByBrandPanel").mouseenter(ghMenuClearTimeout);
			$("#snShopByBrandPanel").mouseleave(ghMenuSetTimeout);
		});
		function snShopByBrandHideMenu() {
			$("#snShopByBrand").removeClass("snMenu_on");
			$("#snShopByBrandPanel").css("visibility", "");
		}
