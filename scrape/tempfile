<script type="text/javascript" language="JavaScript">

/*\
|*|
|*|  :: cookies.js ::
|*|
|*|  A complete cookies reader/writer framework with full unicode support.
|*|
|*|  Revision #1 - September 4, 2014
|*|
|*|  https://developer.mozilla.org/en-US/docs/Web/API/document.cookie
|*|  https://developer.mozilla.org/User:fusionchess
|*|
|*|  This framework is released under the GNU Public License, version 3 or later.
|*|  http://www.gnu.org/licenses/gpl-3.0-standalone.html
|*|
|*|  Syntaxes:
|*|
|*|  * docCookies.setItem(name, value[, end[, path[, domain[, secure]]]])
|*|  * docCookies.getItem(name)
|*|  * docCookies.removeItem(name[, path[, domain]])
|*|  * docCookies.hasItem(name)
|*|  * docCookies.keys()
|*|
\*/

var docCookies = {
  getItem: function (sKey) {
    //if (!sKey) { return null; }
    //return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
	var results = document.cookie.match('(^|;) ?' + sKey + '=([^;]*)(;|$)');
	if (results) {
		return (unescape(results[2]));
	} else {
		return null;
	}	
  },
  setItem: function (sKey, sValue, vEnd, sPath, sDomain, bSecure) {
    if (!sKey || /^(?:expires|max\-age|path|domain|secure)$/i.test(sKey)) { return false; }
    var sExpires = "";
    if (vEnd) {
      switch (vEnd.constructor) {
        case Number:
          sExpires = vEnd === Infinity ? "; expires=Tue, 19 Jan 2038 03:14:07 GMT" : "; max-age=" + vEnd;
          break;
        case String:
          sExpires = "; expires=" + vEnd;
          break;
        case Date:
          sExpires = "; expires=" + vEnd.toUTCString();
          break;
      }
    }
    document.cookie = encodeURIComponent(sKey) + "=" + encodeURIComponent(sValue) + sExpires + (sDomain ? "; domain=" + sDomain : "") + (sPath ? "; path=" + sPath : "") + (bSecure ? "; secure" : "");
    return true;
  },
  removeItem: function (sKey, sPath, sDomain) {
    if (!this.hasItem(sKey)) { return false; }
    document.cookie = encodeURIComponent(sKey) + "=; expires=Thu, 01 Jan 2199 00:00:00 GMT" + (sDomain ? "; domain=" + sDomain : "") + (sPath ? "; path=" + sPath : "");
    return true;
  },
  hasItem: function (sKey) {
    if (!sKey) { return false; }
    return (new RegExp("(?:^|;\\s*)" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\$&") + "\\s*\\=")).test(document.cookie);
  },
  keys: function () {
    var aKeys = document.cookie.replace(/((?:^|\s*;)[^\=]+)(?=;|$)|^\s*|\s*(?:\=[^;]*)?(?:\1|$)/g, "").split(/\s*(?:\=[^;]*)?;\s*/);
    for (var nLen = aKeys.length, nIdx = 0; nIdx < nLen; nIdx++) { aKeys[nIdx] = decodeURIComponent(aKeys[nIdx]); }
    return aKeys;
  },
  updateRecentValue : function(currentValue){
	  var updatedValue = null;
		if(currentValue == undefined || currentValue == null || currentValue == "" || currentValue == "null"){
			updatedValue = "1161804";
		} else {
			var patt = /1161804/i;
			if(!patt.test(currentValue)){
				updatedValue = "1161804," + currentValue; 
			} else {
				updatedValue = currentValue;
			}
		}

		var cookieParts = updatedValue.split(",", 10);
		updatedValue = cookieParts.join();

		return updatedValue;
  }
};

$(function() {
	var cookieKey = "recent_items10052";
	
	//var currentCookieValue = getCookie(cookieKey);
	//var currentCookieValue = decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(cookieKey).replace(/[\-\.\+\*]/g, "\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
	
	//setCookie(cookieKey,cookieValue,30 * 24 * 60 * 60,"/webapp/wcs/stores/servlet/");
	//setCookie(cookieKey,cookieValue,30 * 24 * 60 * 60,"/bikes/");
	
	//var expdate = new Date();
	//expdate.setDate(expdate.getDate() + 30 * 24 * 60); 

	if(typeof(Storage) !== "undefined") {
		var currentLocalStorageValue = null;
		if (localStorage[cookieKey]) {
			currentLocalStorageValue = localStorage[cookieKey]; 
		}

		var storageValue = docCookies.updateRecentValue(currentLocalStorageValue);
		localStorage[cookieKey] = storageValue;

		docCookies.setItem(cookieKey, storageValue,null,"/");
		
	} else {
		var currentCookieValue = docCookies.getItem(cookieKey);
		var cookieValue = docCookies.updateRecentValue(	currentCookieValue);
		
		docCookies.setItem(cookieKey, cookieValue,null,"/");
	}

	
	//docCookies.setItem(cookieKey, cookieValue, Infinity,"/bikes/");
	//docCookies.setItem(cookieKey, cookieValue, Infinity,"/webapp/wcs/stores/servlet/");
	
	//var cookie_exp_date = new Date();
	//cookie_exp_date.setTime(cookie_exp_date.getTime() + 1000 * 60 * 60 * 24 * 30);
	//var documentCookieStr = encodeURIComponent(cookieKey) + "=" + encodeURIComponent(cookieValue) + ";path=/;expires=" + cookie_exp_date.toGMTString();
	//document.cookie=documentCookieStr;
});
</script>
